<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jdjhaha.vocab.vocab.mapper.VocabMapper">

	<select id="selectData" parameterType="hashMap"
		resultType="hashMap">
		select 
				id,
				voca_json,
				username
		from vocab
		where 
		        username=#{username}
		order by id desc
	</select>
	
	<insert id="insertData" parameterType="hashMap">
		insert into vocab(voca_json, username) values(#{voca_json},#{username})
	</insert>
</mapper>  